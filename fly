repeat wait()until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("UpperTorso")and game.Players.LocalPlayer.Character:findFirstChild("Humanoid")local a=game.Players.LocalPlayer:GetMouse()repeat wait()until a;local b=game.Players.LocalPlayer;local c=b.Character.UpperTorso;local d=true;local e=true;local f={f=0,b=0,l=0,r=0}local g={f=0,b=0,l=0,r=0}local h=50;local i=0;function Fly()local j=Instance.new("BodyGyro",c)j.P=9e4;j.maxTorque=Vector3.new(9e9,9e9,9e9)j.cframe=c.CFrame;local k=Instance.new("BodyVelocity",c)k.velocity=Vector3.new(0,0.1,0)k.maxForce=Vector3.new(9e9,9e9,9e9)repeat wait()b.Character.Humanoid.PlatformStand=true;if f.l+f.r~=0 or f.f+f.b~=0 then i=i+.5+i/h;if i>h then i=h end elseif not(f.l+f.r~=0 or f.f+f.b~=0)and i~=0 then i=i-1;if i<0 then i=0 end end;if f.l+f.r~=0 or f.f+f.b~=0 then k.velocity=(game.Workspace.CurrentCamera.CoordinateFrame.lookVector*(f.f+f.b)+game.Workspace.CurrentCamera.CoordinateFrame*CFrame.new(f.l+f.r,(f.f+f.b)*.2,0).p-game.Workspace.CurrentCamera.CoordinateFrame.p)*i;g={f=f.f,b=f.b,l=f.l,r=f.r}elseif f.l+f.r==0 and f.f+f.b==0 and i~=0 then k.velocity=(game.Workspace.CurrentCamera.CoordinateFrame.lookVector*(g.f+g.b)+game.Workspace.CurrentCamera.CoordinateFrame*CFrame.new(g.l+g.r,(g.f+g.b)*.2,0).p-game.Workspace.CurrentCamera.CoordinateFrame.p)*i else k.velocity=Vector3.new(0,0.1,0)end;j.cframe=game.Workspace.CurrentCamera.CoordinateFrame*CFrame.Angles(-math.rad((f.f+f.b)*50*i/h),0,0)until not d;f={f=0,b=0,l=0,r=0}g={f=0,b=0,l=0,r=0}i=0;j:Destroy()k:Destroy()b.Character.Humanoid.PlatformStand=false end;a.KeyDown:connect(function(l)if l:lower()=="e"then if d then d=false else d=true;Fly()end elseif l:lower()=="w"then f.f=1 elseif l:lower()=="s"then f.b=-1 elseif l:lower()=="a"then f.l=-1 elseif l:lower()=="d"then f.r=1 end end)a.KeyUp:connect(function(l)if l:lower()=="w"then f.f=0 elseif l:lower()=="s"then f.b=0 elseif l:lower()=="a"then f.l=0 elseif l:lower()=="d"then f.r=0 end end)Fly()
